{
  "name": "tanstack-start-example-basic",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev:backend": "wrangler dev",
    "build": "vite build && tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,md}",
    "start": "node .output/server/index.mjs",
    "deploy": "npm run build && wrangler deploy",
    "cf-typegen": "wrangler types --env-interface Env",
    "local:init": "wrangler d1 execute stray-tracker --file=./db/schema.sql --local",
    "local:load": "wrangler d1 execute stray-tracker --file=./db/data.sql --local",
    "remote:new": "wrangler d1 create stray-tracker",
    "remote:init": "wrangler d1 execute stray-tracker --remote --file=./db/schema.sql",
    "remote:load": "wrangler d1 execute stray-tracker --remote --file=./db/data-big.sql"
  },
  "dependencies": {
    "@cloudflare/vite-plugin": "^1.13.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-form": "^1.23.4",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-router": "^1.132.19",
    "@tanstack/react-router-devtools": "^1.132.19",
    "@tanstack/react-start": "^1.132.19",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^22.5.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "postcss": "^8.5.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.40.2"
  }
}
